{
  "name": "relo-calc-app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --config vite.config.ts",
    "build": "vite build --config vite.config.ts",
    "preview": "vite preview --config vite.config.ts",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "build:lambdas": "node ./scripts/build-lambdas.mjs",
    "tf:init": "cd terraform && terraform init",
    "tf:val": "cd terraform && terraform validate",
    "tf:plan": "cd terraform && terraform plan",
    "tf:apply": "cd terraform && terraform apply",
    "tf:destroy": "cd terraform && terraform destroy",
    "freeze": "npm ci"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.899.0",
    "@aws-sdk/client-ssm": "^3.895.0",
    "@aws-sdk/client-sts": "^3.899.0",
    "@aws-sdk/credential-provider-ini": "^3.899.0",
    "@aws-sdk/credential-providers": "^3.899.0",
    "@tailwindcss/vite": "^4.1.14",
    "axios": "^1.12.2",
    "daisyui": "^5.3.7",
    "lucide-react": "^0.460.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-error-boundary": "^6.0.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.9.3",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@tailwindcss/postcss": "^4.1.14",
    "@types/node": "^24.6.0",
